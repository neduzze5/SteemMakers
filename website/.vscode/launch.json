    "version": "0.2.0",
    "configurations": [
		{
            "name": "vuejs: chrome",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost/post.php?author=jefpatat&permlink=the-blockchain-truth-about-steem-and-dtube",
            "webRoot": "${workspaceFolder}",
            "breakOnLoad": true
        },
        {
            "name": "Listen for XDebug",
            "type": "php",
            "request": "launch",
            "port": 9000
        },
        {
            "name": "Launch currently open script",
            "type": "php",
            "request": "launch",
            "program": "${file}",
            "cwd": "${fileDirname}",
            "port": 9000
        }
    ]
}